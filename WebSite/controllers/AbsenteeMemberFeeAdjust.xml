select
	"AbsenteeMemberFeeAdjust"."AbsenteeMemberTempID" "AbsenteeMemberTempID"
	,"AbsenteeMemberFeeAdjust"."MemberID" "MemberID"
	,"Member"."MemberCode" "MemberCode"
	,"Member"."NameOfMember" "MemberNameOfMember"
