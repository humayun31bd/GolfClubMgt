  <businessRules>
    <rule id="r100" commandName="Insert|Update" type="Sql" phase="Before"><![CDATA[if (@MemberOfType is null)
set @MemberOfType = 'Full Member'

]]></rule>
    <rule id="r101" commandName="Insert|Update" type="Sql" phase="After"><![CDATA[declare @EntranceFee decimal(18,2), @AnnualFee decimal(18,2), 
@NonMemberCatID int , @MonthlyFee decimal(18,2)

select @EntranceFee=EntranceFee, 
@AnnualFee=AnnualFee, 
@MemberGroupID = MemberGroupID,
@NonMemberCatID=NonMemberCatID
from MemberCategory
Where MemberCategoryID=@MemberCategoryID

Select @MonthlyFee=MonthlyFee
from dbo.MemberFee
Where MemberCategoryID=@MemberCategoryID
And MemberStatusID=@MemberStatusID

set @MonthlyFee= IsNull(@MonthlyFee,0)

Update dbo.MemberInfo
set MemberFee = @EntranceFee, 
MonthlySubcriptionFee=@MonthlyFee,
MemberGroupID = @MemberGroupID,
AnualFee = @AnnualFee, 
NonMemberCatID=@NonMemberCatID
Where MemberID=@MemberID

]]></rule>
    <rule id="r102" commandName="Calculate" type="Sql" phase="Execute"><![CDATA[--debug
declare @EntranceFee decimal(18,2), @AnnualFee decimal(18,2)
--end debug
select @EntranceFee=EntranceFee, @AnnualFee=AnnualFee
from MemberCategory
Where MemberCategoryID=@MemberCategoryID

Set @EntranceFee =IsNull(@EntranceFee,0)
Set @AnualFee = IsNull(@AnnualFee,0)
Set @MemberFee=@EntranceFee
]]></rule>
