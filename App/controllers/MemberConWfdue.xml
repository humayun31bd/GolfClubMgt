      <text><![CDATA[
select
	"MemberConWFDue"."MemberCWFID" "MemberCwfid"
	,"MemberConWFDue"."MemberID" "MemberID"
	,"Member"."NameOfMember" "MemberNameOfMember"
	,"MemberBloodGroup"."BloodGroupName" "MemberBloodGroupName"
