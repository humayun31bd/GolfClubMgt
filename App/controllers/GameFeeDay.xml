<?xml version="1.0" encoding="utf-8"?>
<dataController name="GameFeeDay" conflictDetection="overwriteChanges" label="Game Fee Day" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"GameFeeDay"."GameFeeID" "GameFeeID"
	,"GameFeeDay"."WeekDayID" "WeekDayID"
	,"WeekDay"."WeekDayName" "WeekDayName"
from "dbo"."GameFeeDay" "GameFeeDay"
	left join "dbo"."GameFee" "GameFee" on "GameFeeDay"."GameFeeID" = "GameFee"."GameFeeID"
	left join "dbo"."WeekDay" "WeekDay" on "GameFeeDay"."WeekDayID" = "WeekDay"."WeekDayID"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="GameFeeID" type="Int32" isPrimaryKey="true" label="Game Fee ID" showInSummary="true">
      <items style="Lookup" dataController="GameFee" newDataView="createForm1" dataValueField="GameFeeID" />
    </field>
    <field name="WeekDayID" type="Int32" label="Week Day ID" showInSummary="true">
      <items style="Lookup" dataController="WeekDay" newDataView="createForm1" dataValueField="WeekDayID" dataTextField="WeekDayName" />
    </field>
    <field name="WeekDayName" type="String" readOnly="true" label="Week Day Name" length="50" />
  </fields>
