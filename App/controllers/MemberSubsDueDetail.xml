<?xml version="1.0" encoding="utf-8"?>
<dataController name="MemberSubsDueDetail" conflictDetection="overwriteChanges" label="Member Subs Due Detail" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"MemberSubsDueDetail"."MemberSubsDueDetailID" "MemberSubsDueDetailID"
	,"MemberSubsDueDetail"."MemberSubsDueID" "MemberSubsDueID"
	,"MemberSubsDue"."TotalSubsiDue" "MemberSubsDueTotalSubsiDue"
	,"MemberSubsDue"."TotalAnnualDue" "MemberSubsDueTotalAnnualDue"
	,"MemberSubsDueDetail"."DueDate" "DueDate"
	,"MemberSubsDueDetail"."SubsiDueAmount" "SubsiDueAmount"
	,"MemberSubsDueDetail"."MemberID" "MemberID"
	,"Member"."NameOfMember" "MemberNameOfMember"
	,"MemberSubsDueDetail"."MemberBillID" "MemberBillID"
from "dbo"."MemberSubsDueDetail" "MemberSubsDueDetail"
	left join "dbo"."MemberSubsDue" "MemberSubsDue" on "MemberSubsDueDetail"."MemberSubsDueID" = "MemberSubsDue"."MemberSubsDueID"
	left join "dbo"."MemberInfo" "Member" on "MemberSubsDueDetail"."MemberID" = "Member"."MemberID"
]]></text>
    </command>
    <command id="MemberSubsDueDetailIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="MemberSubsDueDetailID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="MemberSubsDueDetailID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Member Subs Due Detail ID" readOnly="true" />
    <field name="MemberSubsDueID" type="Int32" label="Member Subs Due ID" showInSummary="true">
      <items style="Lookup" dataController="MemberSubsDue" newDataView="createForm1" dataValueField="MemberSubsDueID" dataTextField="TotalSubsiDue" copy="MemberSubsDueTotalAnnualDue=TotalAnnualDue" />
    </field>
    <field name="MemberSubsDueTotalSubsiDue" type="Decimal" readOnly="true" label="Total Subscription Due" />
    <field name="MemberSubsDueTotalAnnualDue" type="Decimal" readOnly="true" label="Total Annual Due" />
    <field name="DueDate" type="DateTime" dataFormatString="MM/yyyy" label="Due Date" showInSummary="true" />
    <field name="SubsiDueAmount" type="Decimal" label="Subscription Due Amount" showInSummary="true" />
    <field name="MemberID" type="Int32" label="Member ID" showInSummary="true">
      <items style="Lookup" dataController="MemberInfo" newDataView="createForm1" dataValueField="MemberID" dataTextField="MemberInfo1NameOfMember" />
    </field>
    <field name="MemberNameOfMember" type="String" readOnly="true" label="Name Of Member" length="150" />
    <field name="MemberBillID" type="Int32" label="Member Bill ID" showInSummary="true" />
  </fields>
